{
    "name": "unibody",
    "version": "2.1.0",
    "icon": "assets/icon.png",
    "displayName": "Unibody",
    "publisher": "YaoweiZou",
    "author": {
        "name": "YaoweiZou"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/YaoweiZou/vscode-extension-unibody"
    },
    "description": "Unibody theme. Support dark and light.",
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Unibody Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/Unibody-Dark.json"
            },
            {
                "label": "Unibody Light",
                "uiTheme": "vs",
                "path": "./themes/Unibody-Light.json"
            }
        ]
    }
}